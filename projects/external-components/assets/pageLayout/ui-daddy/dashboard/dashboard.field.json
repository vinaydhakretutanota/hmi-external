    "project-list": [
        {
            "baseProperties": {
                "id": "openModalAddProject",
                "name": "openModalAddProject",
                "type": "button",
                "formName": "",
                "label": "Add Project"
            },
            "icon": {
                "postIconClass": "fas fa-plus"
            },
            "layoutProperties": {
                "css": {
                    "themeClass": "btn-warning",
                    "customClassName": "add-project__btn",
                    "configuration": {
                        "selectorName": ".add-project__btn",
                        "xs": {
                            "parent-padding": "25px 0 15px 0"
                        }
                    }
                }
            },            
            "expressionProperties": {
                "requiredValue": true,
                "required": "",
                "readOnly": "",
                "readOnlyValue": false,
                "disabled": "",
                "disabledValue": false,
                "visible": "",
                "visibleValue": true
            },
            "events": [
                {
                   "event": "click",
                    "actions" :[ 
                           {
                               "actionType" : "OPEN_MODAL",
                                "showLoading": true,
                                "target": "add-project-modal"
                           }
                    ],
                    "debounce": 0
                }
            ]
            
        },
        {
            "baseProperties": {
                "name": "projectList",
                "id": "projectList",
                "type": "dynamicField",
                "library": "primeng"
            },
            "libraryComponent": "TableExternalComponent",
            "accessor": "",
            "expressionProperties": {
                "requiredValue": false,
                "required": "",
                "readOnly": "",
                "readOnlyValue": false,
                "disabled": "",
                "disabledValue": false,
                "visible": "",
                "visibleValue": true
            },
            "customAttributes": {
              "caption": "My Projects",
              "summary": "",
              "globalFilterFields": ["title","description"],
              "currentPageReportTemplate": "Showing {first} to {last} of {totalRecords} entries",
              "dataKey": "tokenNumber",
              "resizableColumns": true,
              "responsive": true,
              "dataConfig": {
                "name": "projectList",
                "url": "/rest/getProjects",
                "method": "GET",
                "params": {},
                "onSuccess": {
                  "apiDataAccessor": "projects"
                }
              },
              "rows": "5",
              "paginator": "true",
              "columns": [
                { "colName": "Project Name", "accessor": "title" }, 
                { "colName": "Description", "accessor": "description", "colWidth": { "unit": "px", "value": 100 } },
                { "colName": "Github Forked URL", "accessor": "forkUrl", "colWidth": { "unit": "px", "value": 100 } },
                { "colName": "Branch Name", "accessor": "branchName", "colWidth": { "unit": "px", "value": 100 } },
                
                { "colName": "", "html": "<a href='javascript:void(0)'>Build</a>", "action": {
                    "name": "ROW_ACTION",
                    "apiConfig": {
                      "name": "buildProject",
                      "url": "/v1/buildProject",
                      "method": "GET",
                      "payloadParams": [
                          {
                              "paramName": "projectName",
                              "mappedField": "CUSTOM_FIELD_OBJECT",
                              "mappedValue": "projectName"
                          }
                      ],
                      "onSuccess": {
                        "showSuccessMessage": true
                      },
                      "onError": {
                        "showErrorMessage": true
                      }
                    }
                  }, "colWidth": { "unit": "px", "value": 50 } 
                },
                { "colName": "", "html": "<i class='fas fa-download'></i>", "action": {
                    "name": "ROW_ACTION",
                    "apiConfig": {
                      "name": "downloadCode",
                      "url": "/v1/downloadCode",
                      "method": "GET",
                      "payloadParams": [
                          {
                              "paramName": "projectName",
                              "mappedField": "CUSTOM_FIELD_OBJECT",
                              "mappedValue": "projectName"
                          }
                      ],
                      "onSuccess": {
                        "showSuccessMessage": true
                      },
                      "onError": {
                        "showErrorMessage": true
                      }
                    }
                  }, "colWidth": { "unit": "px", "value": 50 } 
                },
                { "colName": "", "html": "<i class='fas fa-edit'></i>", "events": [
                    {
                        "event": "",
                        "actions": [
                            {
                                "actionType" : "OPEN_MODAL",
                                 "showLoading": true,
                                 "target": "edit-project-modal"
                            },
                            {
                                 "actionType": "WAIT_IN_MS",
                                 "timer": 100
                             },
                             {
                                "actionType": "SET_SHARED_DATA",
                                "sharedData": [
                                    {
                                        "varName": "tableRowData",
                                        "staticData": "$ROW_DATA$"
                                    }
                                ]
                            },
                             {
                                 "actionType": "setfield",
                                 "field": "$editProjectDesc$",
                                 "fieldValue": "$SHARED_DATA$.tableRowData.description",
                                 "condition": "true",
                                 "formName": "editProject"
                             },
                             {
                                "actionType": "setfield",
                                "field": "$projectId$",
                                "fieldValue": "$SHARED_DATA$.tableRowData.id",
                                "condition": "true",
                                "formName": "editProject"
                            },
                             {
                                 "actionType": "setfield",
                                 "field": "$editBranchName$",
                                 "fieldValue": "$SHARED_DATA$.tableRowData.branchName",
                                 "condition": "true",
                                 "formName": "editProject"
                             },
                             {
                                "actionType": "setfield",
                                "field": "$editProjectTitle$",
                                "fieldValue": "$SHARED_DATA$.tableRowData.title",
                                "condition": "true",
                                "formName": "editProject"
                            },
                            {
                                "actionType": "setfield",
                                "field": "$editGitUrl$",
                                "fieldValue": "$SHARED_DATA$.tableRowData.forkUrl",
                                "condition": "true",
                                "formName": "editProject"
                            }
                        ],
                        "debounce": 3000
                    }
                ], "colWidth": { "unit": "px", "value": 50 } 
                },
                { "colName": "", "html": "<i class='fas fa-trash'></i>", "action": {
                    "name": "ROW_ACTION",
                    "apiConfig": {
                      "name": "deleteProject",
                      "url": "/rest/project/:projectId",
                      "method": "DELETE",
                      "pathParams": [
                          {
                              "paramName": "projectId",
                              "mappedField": "CUSTOM_FIELD_OBJECT",
                              "mappedValue": "id"
                          }
                      ],
                      "onSuccess": {
                        "deleteTableRow": false,
                        "showSuccessMessage": true,
                        "actions": [
                          {
                            "actionType": "RELOAD_COMPONENT_DATA",
                            "componentName": "projectList"
                          }
                        ]
                      },
                      "onError": {
                        "showErrorMessage": true
                      }
                    }
                  }, "colWidth": { "unit": "px", "value": 50 } 
                }
              ]
            }
        }
    ]
}
